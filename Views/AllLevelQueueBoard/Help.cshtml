
@{
    ViewBag.Title = "Help - Path Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h4>Path Details</h4>
<style>
    .table {
        width: 63%;
        max-width: 100%;
        margin-bottom: 20px;
    }

    element.style {
        color: #337ab7;
    }

    thead th {
        background-color: #337ab7; /* Background color */
        color: #fff; /* Text color */
        border-color: #2e6da4; /* Border color */
    }
</style>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>File Name</th>
            <th>Location</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td><strong>SISubmitted</strong></td>
            <td>@ViewBag.SISubmittedPath</td>
        </tr>
        <tr>
            <td><strong>PRI</strong></td>
            <td>@ViewBag.PRIPath</td>
        </tr>
        <tr>
            <td><strong>PRI Test</strong></td>
            <td>@ViewBag.PRITestPath</td>
        </tr>
        <tr>
            <td><strong>PDF</strong></td>
            <td>@ViewBag.PDFPath</td>
        </tr>
    </tbody>
</table>



